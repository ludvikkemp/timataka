@model IEnumerable<Timataka.Core.Models.Entities.Dicipline>

@{
    ViewData["Title"] = "Diciplines";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Diciplines - Index</h2>

<table class="table">
    <thead>
        <tr>
                <th>
                    Name
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                Edit | Details | Delete - non functional
            </td>
        </tr>
}
    </tbody>
</table>
